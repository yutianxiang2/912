    <script>
        jQuery(document).ready(function () {
            //App.setPage("widgets_box");  //Set current page
            App.init(); //Initialise plugins and elements

            var menu_id = "#" + '@ViewBag.MenuId';
            var submenu_index = '@ViewBag.SubMenuIndex';

            //var div1 = document.getElementById("_RoleManage");
            //div1.style.display = 'none';

            $(".has-sub").removeClass("active");
            $(menu_id).addClass("active");

            $("li.current").removeClass("current");
            $(menu_id).children("ul.sub").children("li").eq(submenu_index).addClass("current");
            $.ajax(
                      {
                          type: "post",
                          url: "/Home/GetUserName",
                          dataType: 'json',
                          success: function (result) {
                              $("#_userName").html(result.message);
                          },

                      }

                 );
            var tip = $("#_TaskCount").text();
            //alert(tip);
            if (tip == null || tip == '' || tip == undefined) ShowTips();
            window.setInterval(ShowTips, 120000);

        });
  

        jQuery(document).ready(function (request, response) {
            $.ajax(
                     {
                         type: "post",
                         url: "/Home/SetMenuHidden",
                         dataType: 'json',
                         success: function (result) {                         
                             response($.map(result.list, function (item) {
                                 //alert(item.menuid);
                                 $("#" + item.menuid).css("display", item.display)
                             }));
                         },
                     });
        });


        function setHomePage() {
            var control = $('#_control').html();
            var action = $('#_action').html();
            //alert(control);
            //alert(action);
            //提交数据库
            $.ajax({
                type: "post",
                url: '@Url.Action("SetHomePage", "Home")',
                data: {
                'controller': control,//ViewContext.RouteData.Values["controller"],
                'action': action,//ViewContext.RouteData.Values["action"]
            },
                dataType: 'json',
                success: function (result) {
                    alert(result.message);
                },
                error: function (result) {
                    console.log(result)
                    try {
                        var json = $.parseJSON(result.responseText);
                        alert(json.errorMessage);
                    } catch (e) {
                        alert(e.message);
                    }
                    return;
                }
            });
        }

        function ShowTips() {
            //alert(1);
            $.ajax(
                     {
                         type: "post",
                         url: "/Home/GetAlert",
                         dataType: 'json',
                         success: function (result) {
                             $("#_TaskCount").html(result.NeedApprovedCount);  //1.待完成任务
                             $("#_RejectCount").html(result.RejectBillingCount);//2.被驳回账单
                             $("#_RejectOrderCount").html(result.RejectOrderCount);//3.被驳回订单
                         },
                     });
        }
    </script>
